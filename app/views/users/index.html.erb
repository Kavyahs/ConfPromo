 <%#= render partial: 'layouts/error_messages', locals: {error_messages: @user.errors.full_messages} if @user.errors.any? -%>
 <div class="row">
  <div class="col-sm-4 col-sm-offset-4">
  <%= form_for @user do |f| %>
  <div class="form-group">
    <%= f.text_field :user_name,:class=>"form-control",:placeholder=>"Your Name",:"data-toggle" => "tooltip", :"data-placement"=> "left", :title=>"Enter Your Name" %>
  </div>
  <div class="form-group">
         <%= f.text_field :email,:class=>"form-control",:placeholder=>"Your Email",:"data-toggle" => "tooltip", :"data-placement"=> "left", :title=>"Enter Your Email"  %>
      </div>
      <div class="form-group">
       <%= f.text_field :organization,:class=>"form-control",:placeholder=>"Organisation",:"data-toggle" => "tooltip", :"data-placement"=> "left", :title=>"Organisation Name" %>
      </div>
      <div class="form-group">
    <%= f.text_field :designation,:class=>"form-control",:placeholder=>"Designation",:"data-toggle" => "tooltip", :"data-placement"=> "left", :title=>"Enter your Designation" %><br>
      </div>
       <div class="form-group btn-margin-top">
        <div class="col-sm-offset-2 col-sm-10">
          <!-- <button type="submit" class="btn btn-primary btn-margin-right">submit</button> -->
          <%= f.submit "submit",:id => "click_disable1",:class => "btn btn-primary btn-margin-right"%>
          <button type="submit" class="btn btn-default">cancel</button>
        </div>
      </div>
    <% end %>
  </div>
</div>

<div style="text-align:right">
<%= link_to "create question",new_question_path,:class => "btn-primary" %>
</div>
<script>
  UserValidator();
</script>
<script>
  function preventBack(){window.history.forward();}
  setTimeout("preventBack()", 0);
  window.onunload=function(){null};
</script>